.SILENT:

CC = g++ -O3
AR = ar
RM = rm -f
TOUCH = touch

WHERE=`pwd`/

OPTIONS = 
MFLAGS = all

OBJECT_DIR = ../obj/
LIBRARY_DIR = ../calmlib/lib/
INCLUDE_DIR = -I$(WHERE)$(LEVEL)../calmlib/include/ -I$(WHERE)$(LEVEL)../include/
LO = $(WHERE)$(LEVEL)$(OBJECT_DIR)
LL = $(WHERE)$(LEVEL)$(LIBRARY_DIR)

LIBDIRS = -L$(LL)

LIBS = -lcalm


